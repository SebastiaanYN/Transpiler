[
  {
    "type": "identifier",
    "match": "[a-zA-Z]\\w*"
  },
  {
    "type": "number",
    "match": "\\d+"
  },
  {
    "type": "operator",
    "match": "[+\\-*\\/^=]"
  },
  {
    "type": "newline",
    "match": "[\\r\\n]+",
    "ignore": true
  },
  {
    "type": "space",
    "match": " ",
    "ignore": true
  },
  {
    "type": "parentheses_open",
    "match": "\\("
  },
  {
    "type": "parentheses_close",
    "match": "\\)"
  }
]
